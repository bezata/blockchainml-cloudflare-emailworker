{
  "name": "blockchainml-mailworker",
  "version": "1.0.0",
  "description": "BlockchainML Email Worker System",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check && npm run test",
    "clean": "rimraf dist .wrangler node_modules",
    "docs": "typedoc --out docs src",
    "security": "npm audit && snyk test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.682.0",
    "@hono/swagger-ui": "^0.4.1",
    "@hono/zod-openapi": "^0.16.4",
    "@upstash/redis": "^1.34.3",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "hono": "^3.0.0",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.4.1",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mongodb": "^6.10.0",
    "nanoid": "^5.0.4",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.8.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "snyk": "^1.1268.0",
    "supertest": "^6.3.4",
    "typedoc": "^0.25.7",
    "typescript": "^5.6.3",
    "wrangler": "^3.83.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "email",
    "worker",
    "cloudflare",
    "blockchain",
    "ml"
  ],
  "author": "Bezata <bezata@blockchainml.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezata/blockchainml-mailworker.git"
  },
  "bugs": {
    "url": "https://github.com/bezata/blockchainml-mailworker/issues"
  },
  "homepage": "https://github.com/bezata/blockchainml-mailworker#readme"
}
